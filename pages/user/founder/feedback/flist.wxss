.flist-item{
	width: 100%;
	height: 2.5rem;
	line-height: 2.5rem;
	color:black;
	padding-left: 1rem;
	border-bottom: 1px solid gray;
}
.flist-item-flex{
	display: flex;
}
.flist-item-flex .flist-item-count{
	padding: 0.7rem;
}
.hadFeedUser,.notFeedUser{
	padding-left: 1rem;
	font-size: 0.8rem;
}
.feedUserContent{
	padding: 0.8rem;
	padding: 0.5rem;
	border: 1px solid black;
	margin-bottom: 1rem;
	width: 98%;
	margin-top: 1rem;
	box-shadow: 1px 1px 1px gray;
}
.feedUserContent::before{
	content: "\e646";
}